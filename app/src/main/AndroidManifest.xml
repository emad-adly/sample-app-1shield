<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emadadly.sampleapplication1shield" >

    <uses-feature android:name="android.hardware.senor.accelerometer"/>
    <uses-feature android:name="android.hardware.senor.magnetic-field"/>
    <uses-feature android:name="android.hardware.senor.proximity"/>
    <uses-feature android:name="android.hardware.senor.orientation"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SensorsList"
            android:label="@string/title_activity_sensors_list">
        </activity>
    </application>

</manifest>
